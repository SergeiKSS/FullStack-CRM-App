{
  "name": "fullstack-crm-app",
  "version": "1.0.0",
  "description": "FullStack-CRM-App",
  "main": "index.js",
  "engines": {
    "node": "9.11.1",
    "npm": "6.0.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-prebuild": "npm install --production=false",
    "heroku-postbuild": "NPM_CONFIG_PRODACTION=false npm run client-install && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergeiKSS/FullStack-CRM-App.git"
  },
  "keywords": [
    "FullStack-CRM-App"
  ],
  "author": "Siarhei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SergeiKSS/FullStack-CRM-App/issues"
  },
  "homepage": "https://github.com/SergeiKSS/FullStack-CRM-App#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  }
}
